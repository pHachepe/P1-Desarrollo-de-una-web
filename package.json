{
  "name": "pec1",
  "version": "1.0.0",
  "description": "",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "npm-run-all clean parcel:dev",
    "build": "npm-run-all clean parcel:build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "parcel:dev": "parcel serve src/pages/*.html --open",
    "parcel:build": "parcel build src/pages/*.html --dist-dir dist --public-url ./",
    "clean": "rimraf dist .parcel-cache",
    "predeploy": "rm -rf dist && parcel build ./*.html --public-url https://phachepe.github.io/P1-Desarrollo-de-una-web",
    "deploy": "gh-pages -d dist"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-js": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "gh-pages": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.12.0",
    "posthtml-include": "^2.0.1",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2"
  }
}
